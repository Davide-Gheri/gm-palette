{
  "name": "@davidegheri/gm-palette",
  "version": "1.0.1",
  "description": "Dominant color and palette using graphicsmagick.",
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "pack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davide-Gheri/gm-palette.git"
  },
  "keywords": [
    "dominant color",
    "colors",
    "top colors",
    "palette",
    "image colors",
    "gm colors",
    "gm palette",
    "graphicsmagick"
  ],
  "contributors": [
    "Akash Dathan",
    "Davide Gheri"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Davide-Gheri/gm-palette/issues"
  },
  "homepage": "https://github.com/Davide-Gheri/gm-palette#readme",
  "dependencies": {
    "gm": "^1.23.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.3.4",
    "@pika/plugin-build-node": "^0.3.13",
    "@pika/plugin-ts-standard-pkg": "^0.3.13",
    "@types/gm": "^1.18.2",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.92",
    "@types/node": "^9.3.0",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "!index.ts",
      "!types.ts"
    ],
    "coverageDirectory": "../coverage"
  }
}
